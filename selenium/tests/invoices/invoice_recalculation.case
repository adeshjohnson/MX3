<tr>
	<td>open</td>
	<td>/billing/functions/settings</td>
	<td></td>
</tr>
<tr>
	<td>type</td>
	<td>invoice_number_start</td>
	<td>INV</td>
</tr>
<tr>
	<td>type</td>
	<td>invoice_number_length</td>
	<td>9</td>
</tr>
<tr>
	<td>click</td>
	<td>invoice_number_type_1</td>
	<td></td>
</tr>
<tr>
	<td>type</td>
	<td>prepaid_invoice_number_start</td>
	<td>INV</td>
</tr>
<tr>
	<td>type</td>
	<td>prepaid_invoice_number_length</td>
	<td>9</td>
</tr>
<tr>
	<td>click</td>
	<td>prepaid_invoice_number_type_1</td>
	<td></td>
</tr>
<tr>
	<td>clickAndWait</td>
	<td>commit</td>
	<td></td>
</tr>
<tr>
	<td>verifyTextPresent</td>
	<td>Settings saved</td>
	<td></td>
</tr>
