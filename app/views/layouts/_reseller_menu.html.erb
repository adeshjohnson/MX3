    reseller_menu.add(47,41,'<%= _('Provider_billing') %>','<%= Web_Dir %>/providers/billing');
    <% end %>
    reseller_menu.add(60, 0, '<%= _('Users') %>', '<%= Web_Dir %>/users/list');
    reseller_menu.add(61, 60, '<%= _('Devices') %>', '<%= Web_Dir %>/devices/devices_all');
